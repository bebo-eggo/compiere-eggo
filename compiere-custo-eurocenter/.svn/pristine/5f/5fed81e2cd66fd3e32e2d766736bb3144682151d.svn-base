CREATE TABLE Z_PRODUCTTAX 
(
    "AD_CLIENT_ID" NUMBER(10,0) NOT NULL ENABLE,
    "AD_ORG_ID"    NUMBER(10,0) DEFAULT NULL NOT NULL ENABLE,
    "CREATED" DATE DEFAULT SYSDATE NOT NULL ENABLE,
    "CREATEDBY" NUMBER(10,0) DEFAULT 0 NOT NULL ENABLE,
    "ISACTIVE"  CHAR(1 BYTE) DEFAULT 'Y' NOT NULL ENABLE,
    "UPDATED" DATE DEFAULT SYSDATE NOT NULL ENABLE,
    "UPDATEDBY"       NUMBER(10,0) DEFAULT 0 NOT NULL ENABLE,
    "M_PRODUCT_ID"    NUMBER(10,0) NOT NULL ENABLE,
    "Z_PRODUCTTAX_ID" NUMBER(10,0) NOT NULL ENABLE,
    "VALUE" NVARCHAR2(10) DEFAULT NULL NOT NULL ENABLE,
    "NAME" NVARCHAR2(50) NOT NULL ENABLE,
    CHECK (IsActive IN ('Y','N')) ENABLE,
    CONSTRAINT "FKPRT_KEY" PRIMARY KEY ("Z_PRODUCTTAX_ID"),
    CONSTRAINT "FKPRT_CLIENT" FOREIGN KEY ("AD_CLIENT_ID") REFERENCES AD_CLIENT ("AD_CLIENT_ID") ENABLE,
    CONSTRAINT "FKPRT_ORG" FOREIGN KEY ("AD_ORG_ID") REFERENCES AD_ORG ("AD_ORG_ID") ENABLE,
    CONSTRAINT "FKPRT_PROD" FOREIGN KEY ("M_PRODUCT_ID") REFERENCES M_PRODUCT ("M_PRODUCT_ID") ENABLE
  );