package com.audaxis.compiere.model.proxy;

import org.compiere.model.MOrgInfo;

import com.audaxis.compiere.model.BasicProxy;

public class MOrgInfoProxy extends BasicProxy<MOrgInfo> {

	public MOrgInfoProxy(MOrgInfo po) {
		super(po);
		// TODO Auto-generated constructor stub
	}
	
	public boolean isPOS()
	{
		return getPO().get_ValueAsBoolean("IsPOS");
	}
	
	public boolean isCuisine()
	{
		return getPO().get_ValueAsBoolean("IsEggo");
	}

}
