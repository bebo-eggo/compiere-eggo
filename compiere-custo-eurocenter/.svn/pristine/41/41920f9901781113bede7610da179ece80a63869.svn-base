package com.audaxis.compiere.model.proxy;

import org.compiere.model.MPriceListVersion;

import com.audaxis.compiere.model.BasicProxy;

public class MPriceListVersionProxy extends BasicProxy<MPriceListVersion> {

	public MPriceListVersionProxy(MPriceListVersion po) {
		super(po);
		// TODO Auto-generated constructor stub
	}

	 /** Set IsAlign.
    @param IsAlign IsAlign value */
    public void setIsAlign (boolean IsAlign)
    {
    	getPO().set_Value ("IsAlign", Boolean.valueOf(IsAlign));
        
    }
    
    /** Get IsAlign.
    @return IsAlign value */
    public boolean IsAlign() 
    {
        return getPO().get_ValueAsBoolean("IsAlign");
        
    }
    
	 /** Set Is_ZFolder.
    @param IsAlign IsAlign value */
    public void Is_ZFolder (boolean Is_ZFolder)
    {
    	getPO().set_Value ("Is_ZFolder", Boolean.valueOf(Is_ZFolder));
        
    }
    
    /** Get Is_ZFolder.
    @return Is_ZFolder value */
    public boolean Is_ZFolder() 
    {
        return getPO().get_ValueAsBoolean("Is_ZFolder");
        
    }
}
