	<parameter name="C_AcctSchema_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT SUM(amount_)       AS amount       ,
  SUM(amountCR_)           AS amountCR     ,
  SUM(amountDR_)           AS amountDR     ,
  0                        AS source_amount,
  TRUNC($P{DateFrom})      AS dateacct     ,
  N'-'                     AS description  ,
  N'-'                     AS gl_cat       ,
  SUBSTR(acct_value_,1, 1) AS acct_1       ,
  acct_value_              AS acct_value   ,
  acct_name_               AS acct_name    ,
  NVL(Y_Id_Cpt_To_Val_Name($P{Account_From}),
  (SELECT MIN(Y_Id_Cpt_To_Val_Name(C_ElementValue_ID))
     FROM C_ElementValue ev
  INNER JOIN C_Element e
       ON e.C_Element_ID    = ev.C_Element_ID
  AND e.ElementType         = 'A'
    WHERE ($P{AD_Client_ID} =0
  OR ev.AD_Client_ID        =$P{AD_Client_ID})
  AND ev.IsSummary          ='N'
  AND ev.PostActual         ='Y'
  ) ) AS iFromAcc, -- SRE (ancienneemnt Z_Id_Cpt_To_Val)
  NVL(Y_Id_Cpt_To_Val_Name($P{Account_To}),
  (SELECT MAX(Y_Id_Cpt_To_Val_Name(C_ElementValue_ID))
     FROM C_ElementValue ev
  INNER JOIN C_Element e
       ON e.C_Element_ID    = ev.C_Element_ID
  AND e.ElementType         = 'A'
    WHERE ($P{AD_Client_ID} =0
  OR ev.AD_Client_ID        =$P{AD_Client_ID})
  AND ev.IsSummary          ='N'
  AND ev.PostActual         ='Y'
  ) ) AS iToAcc  , -- SRE (ancienneemnt Z_Id_Cpt_To_Val)
  0   AS iso_code,
  COALESCE(z_account_starting_balance($P{AD_Client_ID}, $P{AD_Org_ID},
  $P{C_AcctSchema_ID}, acct_id_, TRUNC($P{DateFrom})),0) AS starting_balance,
  0                                    AS ending_balance  ,
  (SELECT NVL(Name,' - ')
     FROM AD_Client cli
    WHERE cli.ad_client_id = $P{AD_Client_ID}
  OR $P{AD_Client_ID}      = 0
  )    AS client_name  ,
  N'-' AS org_name     , -- JMS
  0    AS base_curr    ,
  0    AS adTableID    ,
  0    AS RecordID     ,
  NULL AS BPName       ,
  N'-' AS ORG_NAME_ACCT, -- JMS
  NVL(
  ( SELECT AD_LANGUAGE FROM ad_client WHERE ad_client_id = $P{AD_Client_ID}
  ), 'en_US' ) AS AD_LANGUAGE,
  AccountType_ AS AccountType
   FROM (
  (
  -- selection de lensemble des comptes (un jour mouvementÃ©)
   SELECT 0                AS amount_    ,
    0                      AS amountCR_  ,
    0                      AS amountDR_  ,
    acct.c_elementvalue_id AS acct_id_   ,
    acct.value             AS acct_value_,
    acct.name              AS acct_name_ ,
    acct.AccountType       AS AccountType_
     FROM c_elementvalue acct
  INNER JOIN c_element e
       ON e.c_element_id = acct.c_element_id
  AND e.elementtype      = 'A'
    WHERE EXISTS
    (SELECT *
       FROM fact_acct f
      WHERE acct.c_elementvalue_id                 = f.account_id
    AND f.postingtype                              = 'A'
    AND ( f.c_acctschema_id                        = $P{C_AcctSchema_ID}
    OR $P{C_AcctSchema_ID}                         = 0
    OR $P{C_AcctSchema_ID}                        IS NULL )
    AND (f.ad_client_id                            = $P{AD_Client_ID}
    OR $P{AD_Client_ID}                            = 0)
    AND (f.ad_Org_id                               = $P{AD_Org_ID}
    OR $P{AD_Org_ID}                               = 0)
    AND ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) >= Z_Id_Cpt_To_Val($P{Account_From})
    OR $P{Account_From}                            = 0
    OR $P{Account_From}                            = NULL)
    AND ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) <= Z_Id_Cpt_To_Val($P{Account_To})
    OR $P{Account_To}                              = 0
    OR $P{Account_To}                              = NULL)
    )
  )

UNION ALL
  (
  -- selection de lensemble des mouvements
   SELECT COALESCE(COALESCE(f.amtacctdr,0) - COALESCE(f.amtacctcr,0),0) AS amount_    ,
    COALESCE(f.amtacctcr, 0)                                            AS amountCR_  ,
    COALESCE(f.amtacctdr, 0)                                            AS amountDR_  ,
    acct.c_elementvalue_id                                              AS acct_id_   ,
    acct.value                                                          AS acct_value_,
    acct.name                                                           AS acct_name_ ,
    acct.AccountType                                                    AS AccountType_
     FROM fact_acct f
  INNER JOIN c_elementvalue acct
       ON acct.c_elementvalue_id = f.account_id
  LEFT OUTER JOIN c_period per
       ON per.c_period_id                        = f.c_period_id
  AND (per.periodtype                            ='S'
  OR $P{IncludeAdjPeriod}                        ='Y')
    WHERE ( f.ad_client_id                       = $P{AD_Client_ID}
  OR $P{AD_Client_ID}                            = 0 )
  AND f.postingtype                              = 'A'
  AND ( f.c_acctschema_id                        = $P{C_AcctSchema_ID}
    OR $P{C_AcctSchema_ID}                         = 0
    OR $P{C_AcctSchema_ID}                        IS NULL )
  AND ( f.ad_org_id                              = $P{AD_Org_ID}
  OR $P{AD_Org_ID}                               = 0 )
  AND ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) >= Z_Id_Cpt_To_Val($P{Account_From})
  OR $P{Account_From}                            = 0
  OR $P{Account_From}                            = NULL)
  AND ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) <= Z_Id_Cpt_To_Val($P{Account_To})
  OR $P{Account_To}                              = 0
  OR $P{Account_To}                              = NULL)
  AND f.dateacct                                >= TRUNC($P{DateFrom})
  AND f.dateacct                                <= TRUNC($P{DateTo})
  ) )
GROUP BY acct_id_,
  acct_value_    ,
  acct_name_     ,
  AccountType_
ORDER BY acct_value]]>
	</queryString>
