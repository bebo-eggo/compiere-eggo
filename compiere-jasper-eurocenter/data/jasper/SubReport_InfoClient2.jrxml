<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="SubReport_Org" pageWidth="249" pageHeight="80" columnWidth="249" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" isFloatColumnFooter="true" resourceBundle="traduction">
	<property name="ireport.zoom" value="2.5937424601000023"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="C_BPartner_Location_ID" class="java.math.BigDecimal"/>
	<parameter name="AD_Language" class="java.lang.String"/>
	<parameter name="AD_CLIENT_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[BigDecimal.valueOf(1000000)]]></defaultValueExpression>
	</parameter>
	<parameter name="URL" class="java.lang.String">
		<defaultValueExpression><![CDATA["."]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT
(cgt.Name)  as GREETING,
nvl(cb.value,' ')         as CBVALUE,
nvl(cb.name,' ')          AS clientName,
nvl(cb.name2,' ')          AS clientFirstName,
nvl(locFact.Address1,' ') as INV_ADDRESS1,
nvl(locFact.Address2,' ') as INV_ADDRESS2,
nvl(locFact.Address4,' ') as INV_ADDRESS4,
nvl(locFact.Address3,' ') as INV_ADDRESS3,
nvl(locFact.postal,' ')   as INV_POSTAL,
nvl(locFact.city,' ')     as INV_CITY,
nvl(cc.countrycode,' ')   as countrycode,
nvl(au.phone,' ')         as PHONE,
nvl(au.phone2,' ')        as PHONE2, cb.TAXID
from C_BPartner cb
inner join c_bpartner_location cblFact on (cblFact.c_bpartner_ID = cb.c_bpartner_ID)
inner join c_location locFact on (locFact.c_location_id = cblfact.c_location_id)
inner join c_Country cc on (cc.C_Country_ID = locFact.C_Country_ID)
left join AD_User au on (au.C_BPartner_ID = cb.C_BPartner_ID)
left join c_greeting cg on (cg.c_greeting_id = cb.c_greeting_id)
left join c_greeting_trl cgt on (cgt.c_greeting_id = cg.c_greeting_id
and UPPER(cgt.AD_Language) = UPPER($P{AD_Language}))
where cblFact.c_bpartner_location_ID=$P{C_BPartner_Location_ID}  and rownum=1
union
SELECT DISTINCT
cgt.Name  as GREETING,
nvl(cb.value,' ')         as CBVALUE,
nvl(cb.name,' ')          AS clientName,
nvl(cb.name2,' ')          AS clientFirstName,
nvl(locFact.Address1,' ') as INV_ADDRESS1,
nvl(locFact.Address2,' ') as INV_ADDRESS2,
nvl(locFact.Address4,' ') as INV_ADDRESS4,
nvl(locFact.Address3,' ') as INV_ADDRESS3,
nvl(locFact.postal,' ')   as INV_POSTAL,
nvl(locFact.city,' ')     as INV_CITY,
nvl(cc.countrycode,' ')   as countrycode,
nvl(au.phone,' ')         as PHONE,
nvl(au.phone2,' ')        as PHONE2, cb.TAXID
from C_BPartner cb
inner join c_bpartner_location cblFact on (cblFact.c_bpartner_ID = cb.c_bpartner_ID)
inner join c_location locFact on (locFact.c_location_id = cblfact.c_location_id)
inner join c_Country cc on (cc.C_Country_ID = locFact.C_Country_ID)
left join AD_User au on (au.C_BPartner_ID = cb.C_BPartner_ID)
left join c_greeting cg on (cg.c_greeting_id = cb.c_greeting_id)
left join c_greeting_trl cgt on (cgt.c_greeting_id = cg.c_greeting_id
and UPPER(cgt.AD_Language) = UPPER($P{AD_Language}))
where cblFact.c_bpartner_location_ID=$P{C_BPartner_Location_ID}  and rownum=1
and (locFact.Address3 is not null or locFact.Address4 is not null)]]>
	</queryString>
	<field name="GREETING" class="java.lang.String"/>
	<field name="CBVALUE" class="java.lang.String"/>
	<field name="CLIENTNAME" class="java.lang.String"/>
	<field name="CLIENTFIRSTNAME" class="java.lang.String"/>
	<field name="INV_ADDRESS1" class="java.lang.String"/>
	<field name="INV_ADDRESS2" class="java.lang.String"/>
	<field name="INV_ADDRESS4" class="java.lang.String"/>
	<field name="INV_ADDRESS3" class="java.lang.String"/>
	<field name="INV_POSTAL" class="java.lang.String"/>
	<field name="INV_CITY" class="java.lang.String"/>
	<field name="COUNTRYCODE" class="java.lang.String"/>
	<field name="PHONE" class="java.lang.String"/>
	<field name="PHONE2" class="java.lang.String"/>
	<field name="TAXID" class="java.lang.String"/>
	<title>
		<band height="80">
			<textField isStretchWithOverflow="true" isBlankWhenNull="false">
				<reportElement key="textField" x="0" y="20" width="249" height="60"/>
				<box>
					<topPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<leftPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<bottomPen lineWidth="0.0" lineColor="#000000"/>
					<rightPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
				</box>
				<textElement verticalAlignment="Bottom" lineSpacing="Single" markup="styled">
					<font fontName="Avenir Light" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{INV_ADDRESS1}+" "+$F{INV_ADDRESS2}+ $F{INV_ADDRESS3}+" "+$F{INV_ADDRESS4}+ "\n"+
$F{INV_POSTAL}+" "+ $F{INV_CITY}+"\n"+
($F{TAXID}!=null?("<b>"+$R{TAXIDNo}+"</b>"+$F{TAXID}):"")]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="false">
				<reportElement key="textField" x="0" y="0" width="249" height="20"/>
				<box>
					<topPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<leftPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<bottomPen lineWidth="0.0" lineColor="#000000"/>
					<rightPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
				</box>
				<textElement verticalAlignment="Top" lineSpacing="Single">
					<font fontName="Avenir Heavy" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[($F{GREETING}!=null?$F{GREETING}+" ":"")+$F{CLIENTNAME}+ " " +  ($P{AD_CLIENT_ID}.intValue() == 1000000 ? "" :$F{CLIENTFIRSTNAME})]]></textFieldExpression>
			</textField>
		</band>
	</title>
</jasperReport>
