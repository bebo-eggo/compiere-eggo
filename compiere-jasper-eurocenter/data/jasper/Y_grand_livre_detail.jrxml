	<queryString>
		<![CDATA[select
0 as amount,
0 as amountCR,
0 as amountDR,
0 as source_amount,
trunc($P{DateFrom}) as dateacct,
N'-' as description,
0 as fact_acct_id,
N'-' as gl_cat,
acct.value as acct_value,
acct.name as acct_name,
NVL(Y_Id_Cpt_To_Val_Name($P{Account_From}),
  (SELECT MIN(Y_Id_Cpt_To_Val_Name(C_ElementValue_ID)) FROM C_ElementValue ev INNER JOIN C_Element e ON e.C_Element_ID = ev.C_Element_ID and e.ElementType = 'A' WHERE ($P{AD_Client_ID} =0 OR ev.AD_Client_ID=$P{AD_Client_ID}) AND ev.IsSummary='N' AND ev.PostActual='Y')
  ) as iFromAcc,  -- SRE (ancienneemnt Z_Id_Cpt_To_Val)
NVL(Y_Id_Cpt_To_Val_Name($P{Account_To}),
  (SELECT MAX(Y_Id_Cpt_To_Val_Name(C_ElementValue_ID)) FROM C_ElementValue ev INNER JOIN C_Element e ON e.C_Element_ID = ev.C_Element_ID and e.ElementType = 'A' WHERE ($P{AD_Client_ID} =0 OR ev.AD_Client_ID=$P{AD_Client_ID}) AND ev.IsSummary='N' AND ev.PostActual='Y')
  ) as iToAcc, -- SRE (ancienneemnt Z_Id_Cpt_To_Val)
bse.iso_code as iso_code,
coalesce(z_account_starting_balance(acct.ad_client_id, $P{AD_Org_ID}, $P{C_AcctSchema_ID}, acct.c_elementvalue_id, trunc($P{DateFrom})),0) as starting_balance,
0 as ending_balance,
ac.name as client_name,
decode(org.value, '0', org.name, org.value || ' ' || org.name) as org_name, -- JMS
bse.iso_code as base_curr,
0 as adTableID,
0 as RecordID,
null as BPName,
N'-' as ORG_NAME_ACCT, -- JMS
0 as MatchingNo,
null as MatchingDate,
NVL(( select AD_LANGUAGE from ad_client where ad_client_id = $P{AD_Client_ID} ), 'en_US' ) as AD_LANGUAGE,
0 as devisCR,
0 as devisDR,
0 as numero_ligne,
0 as identique
from c_elementvalue acct
inner join c_element e on e.c_element_id = acct.c_element_id and e.elementtype = 'A'
inner join ad_client ac on ac.ad_client_id = acct.ad_client_id
inner join ad_org org on org.ad_org_id = acct.ad_org_id
inner join c_acctschema cas on cas.c_acctschema_id = $P{C_AcctSchema_ID}
inner join c_currency bse on bse.c_currency_id = cas.c_currency_id
where acct.issummary = 'N'
	and ( acct.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0 )
	and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) >= Z_Id_Cpt_To_Val($P{Account_From}) or $P{Account_From} = 0)
	and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) <= Z_Id_Cpt_To_Val($P{Account_To}) or $P{Account_To} = 0)
   	and ( exists (
					select * from fact_acct f where f.dateacct >= trunc($P{DateFrom})
					and f.dateacct <= trunc($P{DateTo})
					and acct.c_elementvalue_id = f.account_id and f.postingtype = 'A'
					and (f.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0)
					and (f.ad_Org_id = $P{AD_Org_ID} or  $P{AD_Org_ID} = 0)))

union

select
t1.amount,
t1.amountCR,
t1.amountDR,
t1.source_amount,
t1.dateacct,
t1.description,
t1.fact_acct_id,
t1.gl_cat,
t1.acct_value,
t1.acct_name,
t1.iFromAcc,
t1.iToAcc,
t1.iso_code,
t1.starting_balance,
t1.ending_balance,
t1.client_name,
t1.org_name,
t1.base_curr,
t1.adTableID,
t1.RecordID,
t1.BPName,
t1.ORG_NAME_ACCT,
t1.MatchingNo,
t1.MatchingDate,
t1.AD_LANGUAGE,
t1.devisCR,
t1.devisDR,
-- row_number () over (PARTITION BY t1.acct_value, t1.amount ORDER BY t1.acct_value, t1.dateacct, t1.iso_code, t1.adTableID, t1.RecordID) as numero_ligne
t1.numero_ligne,
-- row_number () over (PARTITION BY acct.value, coalesce(coalesce(f.amtacctdr,0) - coalesce(f.amtacctcr,0),0)  ORDER BY employee_id) AS numero_ligne
case when t2.dateacct is not null then 1 else 0 end as identique
from
(
select coalesce(coalesce(f.amtacctdr,0) - coalesce(f.amtacctcr,0),0) as amount,
coalesce(f.amtacctcr, 0) as amountCR,
coalesce(f.amtacctdr, 0) as amountDR,
coalesce(coalesce(f.amtsourcedr,0) - coalesce(f.amtsourcecr,0),0) as source_amount,
coalesce(f.dateacct,sysdate) as dateacct,
coalesce(f.description,N'') as description,
f.MatchingNo as MatchingNo,
f.MatchingDate as MatchingDate,
f.fact_acct_id,
coalesce(glc.name,N'') as gl_cat,
acct.value as acct_value,
acct.name as acct_name,
NVL(Y_Id_Cpt_To_Val_Name($P{Account_From}),
  (SELECT MIN(Y_Id_Cpt_To_Val_Name(C_ElementValue_ID)) FROM C_ElementValue ev INNER JOIN C_Element e ON e.C_Element_ID = ev.C_Element_ID and e.ElementType = 'A' WHERE ($P{AD_Client_ID} =0 OR ev.AD_Client_ID=$P{AD_Client_ID}) AND ev.IsSummary='N' AND ev.PostActual='Y')
  ) as iFromAcc,  -- SRE (ancienneemnt Z_Id_Cpt_To_Val)
NVL(Y_Id_Cpt_To_Val_Name($P{Account_To}),
  (SELECT MAX(Y_Id_Cpt_To_Val_Name(C_ElementValue_ID)) FROM C_ElementValue ev INNER JOIN C_Element e ON e.C_Element_ID = ev.C_Element_ID and e.ElementType = 'A' WHERE ($P{AD_Client_ID} =0 OR ev.AD_Client_ID=$P{AD_Client_ID}) AND ev.IsSummary='N' AND ev.PostActual='Y')
  ) as iToAcc, -- SRE (ancienneemnt Z_Id_Cpt_To_Val)
c.iso_code as iso_code,
0 as starting_balance,
0 as ending_balance,
ac.name as client_name,
--org.name as org_name,
decode(org.value, '0', org.name, org.value || ' ' || org.name) as org_name, -- JMS
bse.iso_code as base_curr,
f.AD_TABLE_ID as adTableID,
f.RECORD_ID as RecordID,
bp.name as BPName,
org2.value || ' ' || org2.name as ORG_NAME_ACCT, -- JMS
NVL(( select AD_LANGUAGE from ad_client where ad_client_id = $P{AD_Client_ID} ), 'en_US' ) as AD_LANGUAGE,
f.amtsourcecr as devisCR,
f.amtsourcedr as devisDR,

row_number () over (PARTITION BY acct.value, coalesce(coalesce(f.amtacctdr,0) - coalesce(f.amtacctcr,0),0)  ORDER BY acct.value, coalesce(f.dateacct,sysdate) , c.iso_code , f.AD_TABLE_ID , f.RECORD_ID ) AS numero_ligne

from c_elementvalue acct
inner join c_element e on e.c_element_id = acct.c_element_id and e.elementtype = 'A'
left outer join fact_acct f on f.dateacct >= trunc($P{DateFrom})
		and f.dateacct <= trunc($P{DateTo})
		and acct.c_elementvalue_id = f.account_id and f.postingtype = 'A'
		and f.ad_client_id = $P{AD_Client_ID}
left outer join c_currency c on c.c_currency_id = f.c_currency_id
left outer join gl_category glc on glc.gl_category_id = f.gl_category_id
inner join ad_client ac on ac.ad_client_id = $P{AD_Client_ID}
inner join ad_org org on org.ad_org_id = $P{AD_Org_ID}
inner join ad_org org2 on org2.ad_org_id = f.ad_org_id   -- JMS
inner join c_acctschema cas on cas.c_acctschema_id = $P{C_AcctSchema_ID}
inner join c_currency bse on bse.c_currency_id = cas.c_currency_id
left outer join C_BPARTNER bp on (bp.C_BPARTNER_ID = f.C_BPARTNER_ID)
where acct.issummary = 'N'   AND (f.Z_IsAllocated  = $P{Z_IsAllocated} OR  'Y'=$P{Z_IsAllocated})
	  and ( f.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0 )
      	  and ( acct.ad_client_id = 0 or acct.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0 )
	  and ( f.ad_org_id = $P{AD_Org_ID} or  $P{AD_Org_ID} = 0 )
	  and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) >= Z_Id_Cpt_To_Val($P{Account_From}) or $P{Account_From} = 0)
	  and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) <= Z_Id_Cpt_To_Val($P{Account_To}) or $P{Account_To} = 0)
	  AND ( $P{Z_IsResume} = 'Y' OR ($P{Z_IsResume} = 'N' AND ( f.matchingNO is null OR f.matchingNO = 0  OR (f.matchingNO is not null AND TRUNC(f.matchingdate) > TRUNC($P{DateTo})))))
            and ( exists (
					select * from fact_acct f where f.dateacct >= trunc($P{DateFrom})
					and f.dateacct <= trunc($P{DateTo})
					and acct.c_elementvalue_id = f.account_id and f.postingtype = 'A'
					and (f.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0)
					and (f.ad_org_id = $P{AD_Org_ID} or  $P{AD_Org_ID} = 0)))


	order by acct_value, dateacct, iso_code, adTableID, RecordID
	)t1

left join

(
select coalesce(coalesce(f.amtacctdr,0) - coalesce(f.amtacctcr,0),0) as amount,
coalesce(f.amtacctcr, 0) as amountCR,
coalesce(f.amtacctdr, 0) as amountDR,
coalesce(coalesce(f.amtsourcedr,0) - coalesce(f.amtsourcecr,0),0) as source_amount,
coalesce(f.dateacct,sysdate) as dateacct,
coalesce(f.description,N'') as description,
f.fact_acct_id,
coalesce(glc.name,N'') as gl_cat,
acct.value as acct_value,
acct.name as acct_name,
NVL(Y_Id_Cpt_To_Val_Name($P{Account_From}),
  (SELECT MIN(Y_Id_Cpt_To_Val_Name(C_ElementValue_ID)) FROM C_ElementValue ev INNER JOIN C_Element e ON e.C_Element_ID = ev.C_Element_ID and e.ElementType = 'A' WHERE ($P{AD_Client_ID} =0 OR ev.AD_Client_ID=$P{AD_Client_ID}) AND ev.IsSummary='N' AND ev.PostActual='Y')
  ) as iFromAcc,  -- SRE (ancienneemnt Z_Id_Cpt_To_Val)
NVL(Y_Id_Cpt_To_Val_Name($P{Account_To}),
  (SELECT MAX(Y_Id_Cpt_To_Val_Name(C_ElementValue_ID)) FROM C_ElementValue ev INNER JOIN C_Element e ON e.C_Element_ID = ev.C_Element_ID and e.ElementType = 'A' WHERE ($P{AD_Client_ID} =0 OR ev.AD_Client_ID=$P{AD_Client_ID}) AND ev.IsSummary='N' AND ev.PostActual='Y')
  ) as iToAcc, -- SRE (ancienneemnt Z_Id_Cpt_To_Val)
c.iso_code as iso_code,
0 as starting_balance,
0 as ending_balance,
ac.name as client_name,
--org.name as org_name,
decode(org.value, '0', org.name, org.value || ' ' || org.name) as org_name, -- JMS
bse.iso_code as base_curr,
f.AD_TABLE_ID as adTableID,
f.RECORD_ID as RecordID,
bp.name as BPName,
org2.value || ' ' || org2.name as ORG_NAME_ACCT, -- JMS
NVL(( select AD_LANGUAGE from ad_client where ad_client_id = $P{AD_Client_ID} ), 'en_US' ) as AD_LANGUAGE,
CurrencyConvert(f.amtacctcr,bse.C_Currency_ID,c.C_Currency_ID,coalesce(f.dateacct,sysdate),0,$P{AD_Client_ID},$P{AD_Org_ID}) as devisCR,
CurrencyConvert(f.amtacctdr,bse.C_Currency_ID,c.C_Currency_ID,coalesce(f.dateacct,sysdate),0,$P{AD_Client_ID},$P{AD_Org_ID}) as devisDR,

row_number () over (PARTITION BY acct.value, coalesce(coalesce(f.amtacctdr,0) - coalesce(f.amtacctcr,0),0)  ORDER BY acct.value, coalesce(f.dateacct,sysdate) , c.iso_code , f.AD_TABLE_ID , f.RECORD_ID ) AS numero_ligne


from c_elementvalue acct
inner join c_element e on e.c_element_id = acct.c_element_id and e.elementtype = 'A'
left outer join fact_acct f on f.dateacct >= trunc($P{DateFrom})
		and f.dateacct <= trunc($P{DateTo})
		and acct.c_elementvalue_id = f.account_id and f.postingtype = 'A'
		and f.ad_client_id = $P{AD_Client_ID}
left outer join c_currency c on c.c_currency_id = f.c_currency_id
left outer join gl_category glc on glc.gl_category_id = f.gl_category_id
inner join ad_client ac on ac.ad_client_id = $P{AD_Client_ID}
inner join ad_org org on org.ad_org_id = $P{AD_Org_ID}
inner join ad_org org2 on org2.ad_org_id = f.ad_org_id   -- JMS
inner join c_acctschema cas on cas.c_acctschema_id = $P{C_AcctSchema_ID}
inner join c_currency bse on bse.c_currency_id = cas.c_currency_id
left outer join C_BPARTNER bp on (bp.C_BPARTNER_ID = f.C_BPARTNER_ID)
where acct.issummary = 'N' AND (f.Z_IsAllocated  = $P{Z_IsAllocated} OR  'Y'=$P{Z_IsAllocated})
	  and ( f.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0 )
      and ( acct.ad_client_id = 0 or acct.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0 )
	  and ( f.ad_org_id = $P{AD_Org_ID} or  $P{AD_Org_ID} = 0 )
	  and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) >= Z_Id_Cpt_To_Val($P{Account_From}) or $P{Account_From} = 0)
	  and ( Z_Id_Cpt_To_Val(acct.c_elementvalue_id) <= Z_Id_Cpt_To_Val($P{Account_To}) or $P{Account_To} = 0)
	  AND ( $P{Z_IsResume} = 'Y' OR ($P{Z_IsResume} = 'N' AND ( f.matchingNO is null OR f.matchingNO = 0  OR (f.matchingNO is not null AND TRUNC(f.matchingdate) > TRUNC($P{DateTo})))))
          and ( exists (
					select * from fact_acct f where f.dateacct >= trunc($P{DateFrom})
					and f.dateacct <= trunc($P{DateTo})
					and acct.c_elementvalue_id = f.account_id and f.postingtype = 'A'
					and (f.ad_client_id = $P{AD_Client_ID} or  $P{AD_Client_ID} = 0)
					and (f.ad_org_id = $P{AD_Org_ID} or  $P{AD_Org_ID} = 0)))


	order by acct_value, dateacct, iso_code, adTableID, RecordID
	)t2  on t1.acct_value = t2.acct_value and t1.amount=-(t2.amount) and t1.numero_ligne=t2.numero_ligne

	order by acct_value, dateacct, iso_code, adTableID, RecordID]]>
	</queryString>
