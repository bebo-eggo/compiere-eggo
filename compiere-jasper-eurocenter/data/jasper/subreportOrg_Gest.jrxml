<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="subreportOrg_Gest" language="groovy" pageWidth="250" pageHeight="109" columnWidth="250" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" resourceBundle="traduction">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="AD_ORG_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[Select
org.name as AD_ORG_NAME,
cl.address1 as AD_ORG_ADDRESS1,
cl.postal as AD_ORG_POSTAL,
cl.city as AD_ORG_CITY,
orgi.telgest,
nvl(orgi.email,' ') as email
from AD_Org org
inner join ad_orginfo orgi on (orgi.ad_org_id = org.AD_Org_ID)
left join C_Location cl on (cl.c_location_id = orgi.c_location_id)
where org.ad_org_id = $P{AD_ORG_ID}]]>
	</queryString>
	<field name="AD_ORG_NAME" class="java.lang.String"/>
	<field name="AD_ORG_ADDRESS1" class="java.lang.String"/>
	<field name="AD_ORG_POSTAL" class="java.lang.String"/>
	<field name="AD_ORG_CITY" class="java.lang.String"/>
	<field name="TELGEST" class="java.lang.String"/>
	<field name="EMAIL" class="java.lang.String"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band height="90" splitType="Stretch">
			<textField isBlankWhenNull="false">
				<reportElement key="textField" x="0" y="0" width="250" height="90"/>
				<box>
					<topPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<leftPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<bottomPen lineWidth="0.0" lineColor="#000000"/>
					<rightPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
				</box>
				<textElement lineSpacing="Single">
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[($F{AD_ORG_NAME} == null ? "" :$F{AD_ORG_NAME}+ '\n') +
($F{AD_ORG_ADDRESS1} == null ? "" :$F{AD_ORG_ADDRESS1}+ '\n' )+
($F{AD_ORG_POSTAL} == null ? "" :$F{AD_ORG_POSTAL}+ ' ' )+
($F{AD_ORG_CITY} == null ? "" :$F{AD_ORG_CITY}+ '\n') +
($F{TELGEST} == null ? "" :$F{TELGEST}) + '\n' +
($F{EMAIL} == null || $F{EMAIL}.trim().equalsIgnoreCase("") ? "" : "Gest" + $F{EMAIL})]]></textFieldExpression>
			</textField>
		</band>
	</title>
</jasperReport>
