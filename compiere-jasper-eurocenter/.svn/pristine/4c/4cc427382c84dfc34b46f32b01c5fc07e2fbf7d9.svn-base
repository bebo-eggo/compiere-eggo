	<parameter name="C_AcctSchema_ID" class="java.lang.Double"/>
	<queryString>
		<![CDATA[SELECT NVL(
  ( SELECT name FROM ad_org WHERE ad_org_id = $P{AD_Org_ID}
  ), ' ' ) AS L_ORG,
  NVL(
  ( SELECT name FROM c_bp_group WHERE c_bp_group_id = $P{C_BP_Group_ID}
  ), ' ' ) AS L_GROUP,
  NVL(
  (SELECT name FROM c_bpartner WHERE c_bpartner_id = $P{C_BPartner_ID}
  ), ' ' )      AS L_BP,
  MAX(bp.value) AS bpvalue,
  MAX(bp.name)  AS name,
  MAX(bp.SO_creditlimit) creditLimit,
  N' Solde Depart'    AS doctype,
  NULL                AS documentno,
  NULL                AS acctno,
  TRUNC($P{DateFrom}) AS dateacct,
  0                   AS DOCAMTDB,
  0                   AS DOCAMTCR,
  (Select
    (SELECT NVL( SUM(Z_INVOICE_OPEN_ASOF_BASE_ACTSM(i.C_Invoice_ID,0,TRUNC($P{DateFrom}),$P{C_AcctSchema_ID})) ,0)
    FROM c_invoice i
    WHERE i.c_bpartner_id = bp.c_bpartner_id
    AND TRUNC(i.dateacct) < TRUNC($P{DateFrom})
    AND ( ( i.IsSOTrx     = 'Y'
    AND $P{IsSOTrx}       = 'Y' )
    OR ( i.IsSOTrx        = 'N'
    AND $P{IsSOTrx}       = 'N' ) )
    AND i.docstatus      IN ('CO','CL','RE','VO')
    AND ( i.AD_Org_ID     = $P{AD_Org_ID}
    OR $P{AD_Org_ID}     IS NULL
    OR $P{AD_Org_ID}      = 0 )
    )                                            -
    (SELECT NVL( SUM( CurrencyConvert( (p.PayAmt - NVL(
      (SELECT SUM(( zpaysign(p.isreceipt)        * al.Amount ))
      FROM c_allocationline al
      INNER JOIN c_allocationhdr ah
      ON ah.c_allocationhdr_id = al.c_allocationhdr_id
      WHERE al.c_payment_id    = p.c_payment_id
      AND TRUNC(ah.dateacct)  <= TRUNC($P{DateFrom})
      AND al.isactive          = 'Y'
      GROUP BY al.c_payment_id
      ), 0 ) ), p.C_Currency_ID,
      (SELECT a.C_Currency_ID
      FROM c_acctschema a
      WHERE a.ad_client_id = p.ad_client_id
      AND rownum           = 1
      ) , p.DateAcct, p.C_ConversionType_ID, p.ad_client_id, p.ad_org_id ) ) ,0)
    FROM c_payment p
    WHERE p.c_bpartner_id = bp.c_bpartner_id
    AND TRUNC(p.dateacct) < TRUNC($P{DateFrom})
    AND ( ( p.IsReceipt   = 'Y'
    AND $P{IsSOTrx}       = 'Y' )
    OR ( p.IsReceipt      = 'N'
    AND $P{IsSOTrx}       = 'N' ) )
    AND p.docstatus      IN ('CO','CL','RE','VO')
    AND (p.c_charge_id   IS NULL
    OR p.c_charge_id      = 0)
    AND ( p.AD_Org_ID     = $P{AD_Org_ID}
    OR $P{AD_Org_ID}     IS NULL
    OR $P{AD_Org_ID}      = 0 )
    )
  FROM dual
  )    AS DOCSOLDE,
  NULL AS Extrait,
  NVL(
  ( SELECT AD_LANGUAGE FROM ad_client WHERE ad_client_id = $P{AD_Client_ID}
  ), 'en_US' ) AS AD_LANGUAGE
FROM c_bp_group bpg,
  c_bpartner bp
WHERE ( bpg.Value     >= $P{BP_GROUP_FROM}
OR $P{BP_GROUP_FROM}  IS NULL
OR $P{BP_GROUP_FROM}   = '' )
AND ( bpg.Value       <= $P{BP_GROUP_TO}
OR $P{BP_GROUP_TO}    IS NULL
OR $P{BP_GROUP_TO}     = '' )
AND ( bp.C_BP_Group_ID = bpg.C_BP_Group_ID )
AND ( bp.C_BPartner_ID = $P{C_BPartner_ID}
OR $P{C_BPartner_ID}  IS NULL
OR $P{C_BPartner_ID}   = 0 )
AND ( bp.C_BP_Group_ID = $P{C_BP_Group_ID}
OR $P{C_BP_Group_ID}  IS NULL
OR $P{C_BP_Group_ID}   = 0 )
AND ( (bp.iscustomer   = 'Y'
AND $P{IsSOTrx}        = 'Y')
OR (bp.isvendor        = 'Y'
AND $P{IsSOTrx}        = 'N'))
AND ( bp.ad_client_id  = $P{AD_Client_ID}
OR $P{AD_Client_ID}   IS NULL
OR $P{AD_Client_ID}    = 0 )
GROUP BY bp.C_BPartner_ID
UNION
SELECT NVL(
  ( SELECT name FROM ad_org WHERE ad_org_id = $P{AD_Org_ID}
  ), ' ' ) AS L_ORG,
  NVL(
  (SELECT name FROM c_bp_group WHERE c_bp_group_id = $P{C_BP_Group_ID}
  ), ' ' ) AS L_GROUP,
  NVL(
  (SELECT name FROM c_bpartner WHERE c_bpartner_id = $P{C_BPartner_ID}
  ), ' ' ) AS L_BP,
  bp.value AS bpvalue,
  bp.name  AS name,
  bp.SO_creditlimit creditLimit,
  unistr(j.tablename) AS doctype,
  j.documentno
  || '                           '
  || j.zfact_acct_id AS documentno,
  ev.value           AS acctno,
  TRUNC(fa.dateacct) AS dateacct,
  CASE
    WHEN ($P{IsSOTrx}  = 'Y'
    AND fa.ad_table_id = 318)
    THEN fa.amtacctdr - fa.amtacctcr
    WHEN ($P{IsSOTrx}  = 'N'
    AND fa.ad_table_id = 335)
    THEN -1 * (fa.amtacctdr - fa.amtacctcr)
    WHEN ($P{IsSOTrx}   = 'N'
    AND fa.ad_table_id IN (390,735))
    THEN                -1 * (fa.amtacctdr - fa.amtacctcr)
    ELSE 0
  END AS DOCAMTDB,
  CASE
    WHEN ($P{IsSOTrx}  = 'Y'
    AND fa.ad_table_id = 335)
    THEN fa.amtacctdr - fa.amtacctcr
    WHEN ($P{IsSOTrx}  = 'N'
    AND fa.ad_table_id = 318)
    THEN -1 * (fa.amtacctdr - fa.amtacctcr)
    WHEN ($P{IsSOTrx}   = 'Y'
    AND fa.ad_table_id IN (390,735))
    THEN (fa.amtacctdr  - fa.amtacctcr)
    ELSE 0
  END AS DOCAMTCR,
  0   AS DOCSOLDE,
  CASE
    WHEN (fa.ad_table_id = 335)
    THEN
      (SELECT DISTINCT bs.NAME
      FROM C_BankStatementLine bsl
      INNER JOIN C_BankStatement bs
      ON bs.C_BankStatement_ID = bsl.C_BankStatement_ID
      AND bs.docstatus        IN ('CO','CL')
      WHERE bsl.C_Payment_ID   = fa.record_id
      )
    ELSE NULL
  END AS Extrait,
  NVL(
  (SELECT AD_LANGUAGE
  FROM ad_client
  WHERE ad_client_id = $P{AD_Client_ID}
  ), 'en_US' ) AS AD_LANGUAGE
FROM fact_acct fa
INNER JOIN c_bpartner bp
ON bp.C_BPartner_ID = fa.C_BPartner_ID
INNER JOIN c_bp_group bpg
ON bp.C_BP_Group_ID = bpg.C_BP_Group_ID
LEFT OUTER JOIN gl_category glc
ON glc.gl_category_id = fa.gl_category_id
INNER JOIN c_elementvalue ev
ON ev.c_elementvalue_id = fa.account_id
INNER JOIN z_journal j
ON j.zfact_acct_id       = fa.fact_acct_id
WHERE ( bpg.Value       >= $P{BP_GROUP_FROM}
OR $P{BP_GROUP_FROM}    IS NULL
OR $P{BP_GROUP_FROM}     = '' )
AND ( bpg.Value         <= $P{BP_GROUP_TO}
OR $P{BP_GROUP_TO}      IS NULL
OR $P{BP_GROUP_TO}       = '' )
AND ( bp.C_BPartner_ID   = $P{C_BPartner_ID}
OR $P{C_BPartner_ID}    IS NULL
OR $P{C_BPartner_ID}     = 0 )
AND ( bp.C_BP_Group_ID   = $P{C_BP_Group_ID}
OR $P{C_BP_Group_ID}    IS NULL
OR $P{C_BP_Group_ID}     = 0 )
AND (($P{IsSOTrx}        = 'Y'
AND bp.iscustomer        = 'Y')
OR ($P{IsSOTrx}          = 'N'
AND bp.isvendor          = 'Y'))
AND ( fa.AD_Org_ID       = $P{AD_Org_ID}
OR $P{AD_Org_ID}        IS NULL
OR $P{AD_Org_ID}         = 0 )
AND fa.ad_client_id      = $P{AD_Client_ID}
AND TRUNC(fa.dateacct)  >= TRUNC($P{DateFrom})
AND TRUNC(fa.dateacct)  <= TRUNC($P{DateTo})
AND ( fa.c_acctschema_id = $P{C_AcctSchema_ID}
OR $P{C_AcctSchema_ID}   = 0
OR $P{C_AcctSchema_ID}  IS NULL )
AND (
  -- Facture
  (fa.ad_table_id IN (318)
AND
  -- ev.value like N'4%' Pas bon car on prend les 401 et 4456 (Tva) et 499 facture de reprise)
  -- fa.c_tax_id is null -- Facile ! c est le seul fact acct qui n a pas de tax
  $P{IsSOTrx} =
  (SELECT issotrx FROM c_invoice WHERE c_invoice_id = fa.record_id
  )
AND fa.c_tax_id IS NULL
  -- Chez simop c_tax_id est 0 sur les vieux records 251
AND fa.account_id NOT IN
  (SELECT account_id
  FROM c_validcombination
  WHERE c_validcombination_id IN
    (SELECT t_due_acct FROM c_tax_acct
    )
  )
AND fa.account_id NOT IN
  (SELECT account_id
  FROM c_validcombination
  WHERE c_validcombination_id IN
    (SELECT t_liability_acct FROM c_tax_acct
    )
  )
AND fa.account_id NOT IN
  (SELECT account_id
  FROM c_validcombination
  WHERE c_validcombination_id IN
    (SELECT t_credit_acct FROM c_tax_acct
    )
  )
AND fa.account_id NOT IN
  (SELECT account_id
  FROM c_validcombination
  WHERE c_validcombination_id IN
    (SELECT t_receivables_acct FROM c_tax_acct
    )
  ) )
  -- Paiement
OR (fa.ad_table_id       IN (335)
AND (ev.IsBankTransitAcct = 'Y') -- compte banque
AND $P{IsSOTrx} =
  (SELECT isreceipt
  FROM c_payment
  WHERE c_payment_id = fa.record_id
  )
AND ((SELECT C_Charge_ID FROM C_Payment WHERE c_payment_id = fa.record_id) IS NULL
OR (SELECT C_Charge_ID FROM C_Payment WHERE c_payment_id = fa.record_id)    = 0 ) )
  -- Affectation de Paiement
OR (fa.ad_table_id IN (390,735)
AND ev.AccountType in ('R','E'))
AND $P{IsSOTrx} =
  (SELECT DISTINCT NVL(i.issotrx,p.isreceipt)
  FROM c_allocationline al
  LEFT OUTER JOIN c_invoice i
  ON al.c_invoice_id=i.c_invoice_id
  LEFT OUTER JOIN c_payment p
  ON al.c_payment_id          =p.c_payment_id
  WHERE al.c_allocationhdr_id = fa.record_id
  ) )
ORDER BY 4,
  10,
  7]]>
	</queryString>
