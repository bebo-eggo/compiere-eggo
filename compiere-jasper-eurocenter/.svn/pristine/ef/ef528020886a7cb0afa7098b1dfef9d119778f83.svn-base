	<parameter name="C_BP_Group_ID" class="java.lang.Double" isForPrompting="false">
		<defaultValueExpression><![CDATA[new java.lang.Double(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="C_BPartner_ID" class="java.lang.Double" isForPrompting="false">
		<defaultValueExpression><![CDATA[new java.lang.Double(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="IsSOTrx" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["Y"]]></defaultValueExpression>
	</parameter>
	<parameter name="Summary" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["N"]]></defaultValueExpression>
	</parameter>
	<parameter name="BP_GROUP_FROM" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="BP_GROUP_TO" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["ZZZZZZZZ"]]></defaultValueExpression>
	</parameter>
	<parameter name="excel" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["N"]]></defaultValueExpression>
	</parameter>
	<parameter name="SORTBYNAME" class="java.lang.Double" isForPrompting="false">
		<defaultValueExpression><![CDATA[new Double(5)]]></defaultValueExpression>
	</parameter>
	<parameter name="CURRENT_LANG" class="java.lang.String" isForPrompting="false"/>
	<parameter name="Echu1" class="java.lang.Double" isForPrompting="false">
		<defaultValueExpression><![CDATA[new java.lang.Double(30)]]></defaultValueExpression>
	</parameter>
	<parameter name="Echu2" class="java.lang.Double" isForPrompting="false">
		<defaultValueExpression><![CDATA[new java.lang.Double(60)]]></defaultValueExpression>
	</parameter>
	<parameter name="Echu3" class="java.lang.Double" isForPrompting="false">
		<defaultValueExpression><![CDATA[new java.lang.Double(90)]]></defaultValueExpression>
	</parameter>
	<parameter name="C_AcctSchema_ID" class="java.lang.Double"/>
	<parameter name="Financement" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT NVL(
  ( SELECT name FROM ad_client WHERE ad_client_id = $P{AD_Client_ID}
  ), ' ' ) AS L_CLIENT,
  NVL(
  ( SELECT name FROM ad_org WHERE ad_org_id = $P{AD_Org_ID}
  ), ' ' ) AS L_ORG,
  NVL(
  ( SELECT name FROM c_bp_group WHERE c_bp_group_id = $P{C_BP_Group_ID}
  ), ' ' ) AS L_GROUP,
  NVL(
  (SELECT name FROM c_bpartner WHERE c_bpartner_id = $P{C_BPartner_ID}
  ), ' ' ) AS L_BP             ,
  bp.value AS bpvalue          ,
  bp.name  AS name             ,
  bp.SO_creditlimit creditLimit,
  'I'                                                                                   AS doctype               ,
  i.documentno                                                                          AS documentno            ,
  TRUNC(i.dateinvoiced)                                                                 AS dateinvoiced          ,
  TRUNC(i.dateacct)                                                                     AS dateacct              ,
  i.isindispute                                                                         AS indispute             ,
  PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom}))           AS dueday                ,
  i.dateinvoiced-PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, i.dateinvoiced) AS duedate               ,
  CurrencyConvert(i.Grandtotal, i.C_Currency_ID,
  (SELECT a.C_Currency_ID
     FROM c_acctschema a
    WHERE a.c_acctschema_id = $P{C_AcctSchema_ID}
  ) , i.DateAcct, i.C_ConversionType_ID, i.ad_client_id, i.ad_org_id) AS DOCAMT         ,
  Z_Invoice_Open_AsOf_Base(i.C_Invoice_ID,0,TRUNC($P{DateFrom}) )     AS grandtotalbase ,
  0                                                                   AS PayAmt         ,
  CASE
    WHEN (PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom})) <
      (SELECT $P{Echu1} * (-1) FROM DUAL
      ) )
    THEN Z_Invoice_Open_AsOf_Base(i.C_Invoice_ID,0,TRUNC($P{DateFrom}) )
    ELSE 0
  END NECH30,
  CASE
    WHEN (PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom})) <= 0
    AND PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom}))   >=
      (SELECT $P{Echu1} * (-1) FROM DUAL
      ))
    THEN Z_Invoice_Open_AsOf_Base(i.C_Invoice_ID,0,TRUNC($P{DateFrom}) )
    ELSE 0
  END NECH,
  CASE
    WHEN (PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom})) > 0
    AND PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom}))  <= $P{Echu1})
    THEN Z_Invoice_Open_AsOf_Base(i.C_Invoice_ID,0,TRUNC($P{DateFrom}) )
    ELSE 0
  END R1,
  CASE
    WHEN (PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom})) > $P{Echu1}
    AND PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom}))  <= $P{Echu2})
    THEN Z_Invoice_Open_AsOf_Base(i.C_Invoice_ID,0,TRUNC($P{DateFrom}) )
    ELSE 0
  END R31,
  CASE
    WHEN (PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom})) > $P{Echu2}
    AND PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom}))  <= $P{Echu3})
    THEN Z_Invoice_Open_AsOf_Base(i.C_Invoice_ID,0,TRUNC($P{DateFrom}) )
    ELSE 0
  END R61,
  CASE
    WHEN (PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom})) > $P{Echu3} )
    THEN Z_Invoice_Open_AsOf_Base(i.C_Invoice_ID,0,TRUNC($P{DateFrom}) )
    ELSE 0
  END R90,
  NVL(
  ( SELECT AD_LANGUAGE FROM ad_client WHERE ad_client_id = $P{AD_Client_ID}
  ), 'en_US' )  AS AD_LANGUAGE,
  $P{Echu1} + 1 AS Echu1_2    ,
  $P{Echu2} + 1 AS Echu2_3    ,
  $P{Echu1}     AS Echu1      ,
  $P{Echu2}     AS Echu2      ,
  $P{Echu3}     AS Echu3
   FROM c_invoice i,
  c_bp_group bpg   ,
  c_bpartner bp
  WHERE ( bpg.Value                                                 >= $P{BP_GROUP_FROM}
OR $P{BP_GROUP_FROM}                                                IS NULL
OR $P{BP_GROUP_FROM}                                                 = '' )
AND ( bpg.Value                                                     <= $P{BP_GROUP_TO}
OR $P{BP_GROUP_TO}                                                  IS NULL
OR $P{BP_GROUP_TO}                                                   = '' )
AND ( bp.C_BP_Group_ID                                               = bpg.C_BP_Group_ID )
AND ( bp.C_BPartner_ID                                               = $P{C_BPartner_ID}
OR $P{C_BPartner_ID}                                                IS NULL
OR $P{C_BPartner_ID}                                                 = 0 )
AND ( bp.C_BP_Group_ID                                               = $P{C_BP_Group_ID}
OR $P{C_BP_Group_ID}                                                IS NULL
OR $P{C_BP_Group_ID}                                                 = 0 )
AND ( ( i.issotrx                                                    = 'Y'
AND $P{IsSOTrx}                                                      = 'Y' )
OR ( i.issotrx                                                       = 'N'
AND $P{IsSOTrx}                                                      = 'N' ) )
AND i.docstatus                                                     IN ('CO','CL','RE','VO')
AND ( i.AD_Client_ID                                                 = $P{AD_Client_ID}
OR $P{AD_Client_ID}                                                 IS NULL
OR $P{AD_Client_ID}                                                  = 0 )
AND ( i.AD_Org_ID                                                    = $P{AD_Org_ID}
OR $P{AD_Org_ID}                                                    IS NULL
OR $P{AD_Org_ID}                                                     = 0 )
AND i.c_bpartner_id                                                  = bp.c_bpartner_id
AND TRUNC(i.dateacct)                                               <= TRUNC($P{DateFrom})
AND Z_Invoice_Open_AsOf_Base(i.C_Invoice_ID,0,TRUNC($P{DateFrom}) ) <> 0
AND i.Posted                                                         ='Y'
AND (($P{Financement} = 'N' AND i.IsFinancement = 'N') OR ($P{Financement} = 'F' AND i.IsFinancement = 'Y') OR ($P{Financement} = 'Y'))
  UNION

 SELECT NVL(
  ( SELECT name FROM ad_client WHERE ad_client_id = $P{AD_Client_ID}
  ), ' ' ) AS L_CLIENT,
  NVL(
  ( SELECT name FROM ad_org WHERE ad_org_id = $P{AD_Org_ID}
  ), ' ' ) AS L_ORG,
  NVL(
  (SELECT name FROM c_bp_group WHERE c_bp_group_id = $P{C_BP_Group_ID}
  ), ' ' ) AS L_GROUP,
  NVL(
  (SELECT name FROM c_bpartner WHERE c_bpartner_id = $P{C_BPartner_ID}
  ), ' ' ) AS L_BP             ,
  bp.value AS bpvalue          ,
  bp.name  AS name             ,
  bp.SO_creditlimit creditLimit,
  'P' AS docType               ,
  p.documentno
  || '          '
  || p.c_payment_id                    AS documentno  ,
  TRUNC(p.datetrx)                     AS dateinvoiced,
  TRUNC(p.dateacct)                    AS dateacct    ,
  'N'                                  AS indispute   ,
  0                                    AS dueday      ,
  TRUNC(COALESCE(p.duedate,p.datetrx)) AS duedate     ,
  - CurrencyConvert(p.PayAmt, p.C_Currency_ID,
  (SELECT a.C_Currency_ID
     FROM c_acctschema a
    WHERE a.ad_client_id = p.ad_client_id
  ) , p.DateAcct, p.C_ConversionType_ID, p.ad_client_id, p.ad_org_id) AS DOCAMT,
                                      - CurrencyConvert( (p.PayAmt - NVL(
  (SELECT SUM(( zpaysign(p.isreceipt) * al.Amount ))
     FROM c_allocationline al
  INNER JOIN c_allocationhdr ah
       ON ah.c_allocationhdr_id = al.c_allocationhdr_id
    WHERE al.c_payment_id       = p.c_payment_id
  AND TRUNC(ah.dateacct)       <= TRUNC($P{DateFrom})
  AND al.isactive               = 'Y'
 GROUP BY al.c_payment_id
  ), 0 ) ), p.C_Currency_ID,
  (SELECT a.C_Currency_ID
     FROM c_acctschema a
    WHERE a.c_acctschema_id = $P{C_AcctSchema_ID}
  ) , p.DateAcct, p.C_ConversionType_ID, p.ad_client_id, p.ad_org_id ) AS grandtotalbase,
                                      - CurrencyConvert( (p.PayAmt - NVL(
  (SELECT SUM(( zpaysign(p.isreceipt) * al.Amount ))
     FROM c_allocationline al
  INNER JOIN c_allocationhdr ah
       ON ah.c_allocationhdr_id = al.c_allocationhdr_id
    WHERE al.c_payment_id       = p.c_payment_id
  AND TRUNC(ah.dateacct)       <= TRUNC($P{DateFrom})
  AND al.isactive               = 'Y'
 GROUP BY al.c_payment_id
  ), 0 ) ), p.C_Currency_ID,
  (SELECT a.C_Currency_ID
     FROM c_acctschema a
    WHERE a.c_acctschema_id = $P{C_AcctSchema_ID}
  ) , p.DateAcct, p.C_ConversionType_ID, p.ad_client_id, p.ad_org_id ) AS PayAmt,
  0                                                                    AS NECH30,
  0                                                                    AS NECH  ,
  0                                                                    AS R1    ,
  0                                                                    AS R31   ,
  0                                                                    AS R61   ,
  0                                                                    AS R90   ,
  NVL(
  (SELECT AD_LANGUAGE
     FROM ad_client
    WHERE ad_client_id = $P{AD_Client_ID}
  ), 'en_US' )  AS AD_LANGUAGE,
  $P{Echu1} + 1 AS Echu1_2    ,
  $P{Echu2} + 1 AS Echu2_3    ,
  $P{Echu1}     AS Echu1      ,
  $P{Echu2}     AS Echu2      ,
  $P{Echu3}     AS Echu3
   FROM c_Payment p,
  c_bp_group bpg   ,
  c_bpartner bp
  WHERE ( bpg.Value   >= $P{BP_GROUP_FROM}
OR $P{BP_GROUP_FROM}  IS NULL
OR $P{BP_GROUP_FROM}   = '' )
AND ( bpg.Value       <= $P{BP_GROUP_TO}
OR $P{BP_GROUP_TO}    IS NULL
OR $P{BP_GROUP_TO}     = '' )
AND ( bp.C_BP_Group_ID = bpg.C_BP_Group_ID )
AND ( bp.C_BPartner_ID = $P{C_BPartner_ID}
OR $P{C_BPartner_ID}  IS NULL
OR $P{C_BPartner_ID}   = 0 )
AND ( bp.C_BP_Group_ID = $P{C_BP_Group_ID}
OR $P{C_BP_Group_ID}  IS NULL
OR $P{C_BP_Group_ID}   = 0 )
AND ( ( p.IsReceipt    = 'Y'
AND $P{IsSOTrx}        = 'Y' )
OR ( p.IsReceipt       = 'N'
AND $P{IsSOTrx}        = 'N' ) )
AND p.docstatus       IN ('CO','CL','RE','VO')
AND (p.C_Charge_ID     = 0
OR p.C_Charge_ID      IS NULL)
AND p.c_bpartner_id    = bp.c_bpartner_id
AND ( p.AD_Client_ID   = $P{AD_Client_ID}
OR $P{AD_Client_ID}   IS NULL
OR $P{AD_Client_ID}    = 0 )
AND ( p.AD_Org_ID      = $P{AD_Org_ID}
OR $P{AD_Org_ID}      IS NULL
OR $P{AD_Org_ID}       = 0 )
AND TRUNC(p.dateacct) <= TRUNC($P{DateFrom})
AND CurrencyConvert( (p.PayAmt        - NVL(
  (SELECT SUM(( zpaysign(p.isreceipt) * al.Amount ))
     FROM c_allocationline al
  INNER JOIN c_allocationhdr ah
       ON ah.c_allocationhdr_id = al.c_allocationhdr_id
    WHERE al.c_payment_id       = p.c_payment_id
  AND TRUNC(ah.dateacct)       <= TRUNC($P{DateFrom})
  AND al.isactive               = 'Y'
 GROUP BY al.c_payment_id
  ), 0 ) ), p.C_Currency_ID,
  (SELECT a.C_Currency_ID
     FROM c_acctschema a
    WHERE a.c_acctschema_id = $P{C_AcctSchema_ID}
  ) , p.DateAcct, p.C_ConversionType_ID, p.ad_client_id, p.ad_org_id ) <> 0
AND p.Posted ='Y'
AND $P{Financement} != 'F'
ORDER BY $P!{SORTBYNAME},
  14]]>
	</queryString>
