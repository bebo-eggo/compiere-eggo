	<queryString>
		<![CDATA[Select
NVL(( select name from ad_client where ad_client_id = $P{AD_Client_ID} ), ' ' ) as L_CLIENT,
NVL(( select name from ad_org where ad_org_id = $P{AD_Org_ID} ), ' ' ) as L_ORG,
bp.value as BPValue, bp.name as BPName, pc.name as pcvalue, p.value as pvalue, io.documentno, iol.line,
iol.MOVEMENTQTY as qtereceptionne,
io.movementdate as movementdate,
nvl((select(sum(mi.qty))  from M_MATCHINV mi where trunc(mi.dateacct) <= $P{DateEff} and
mi.M_INOUTLINE_ID = iol.M_INOUTLINE_ID and mi.posted = 'Y' group by mi.M_INOUTLINE_ID),0) as qtefacture,
(iol.MOVEMENTQTY - nvl((select(sum(mi.qty)) from M_MATCHINV mi where trunc(mi.dateacct) <= $P{DateEff} and
mi.M_INOUTLINE_ID = iol.M_INOUTLINE_ID and mi.posted = 'Y' group by mi.M_INOUTLINE_ID),0)) as
qteafacturer,
nvl(fa.AMTACCTCR - fa.AMTACCTDR,0) * (iol.MOVEMENTQTY - nvl((select(NVL(sum(mi.qty),0)) from M_MATCHINV mi where trunc(mi.dateacct) <= $P{DateEff} and
mi.M_INOUTLINE_ID = iol.M_INOUTLINE_ID and mi.posted = 'Y' group by mi.M_INOUTLINE_ID),0)) / iol.movementqty as montantafacturer,
NVL(ppo.vendorproductno, ' ')  as vendorproduct --ZCOM497
, NVL(o.poreference, ' ') as commandefourn,   --ZCOM497
substr(oo.value,0,4) as org_value,
nvl(ol.PriceActual,0) * (iol.MOVEMENTQTY - nvl((select(NVL(sum(mi.qty),0)) from M_MATCHINV mi where trunc(mi.dateacct) <= $P{DateEff} and
mi.M_INOUTLINE_ID = iol.M_INOUTLINE_ID and mi.posted = 'Y' group by mi.M_INOUTLINE_ID),0)) as montantoa,
nvl(o.documentno,' ') as documentoa,
NVL(( select AD_LANGUAGE from ad_client where ad_client_id = $P{AD_Client_ID} ), 'en_US' ) as AD_LANGUAGE
from m_inoutline iol
inner join m_inout io on io.m_inout_id = iol.m_inout_id
inner join m_product p on p.m_product_id = iol.m_product_id
inner join m_product_category pc on pc.m_product_category_id = p.m_product_category_id
left outer join m_product_po ppo on (ppo.m_product_id = p.m_product_id and ppo.c_BPartner_id = io.c_bpartner_id) --ZCOM497
left outer join c_orderline ol on (ol.c_orderline_id = iol.c_orderline_id) --ZCOM497
left outer join c_order o on (o.c_order_id = ol.c_order_id)  --ZCOM497
inner join ad_org oo on oo.ad_org_id = io.ad_org_id
left outer join FACT_ACCT fa on (fa.ad_table_id = 319 and fa.RECORD_ID = iol.M_INOUT_ID and fa.LINE_ID = iol.M_INOUTLINE_ID and fa.AMTACCTCR <> 0 and trunc(fa.dateacct) <= $P{DateEff})
inner join c_bpartner bp on bp.c_bpartner_id = io.c_bpartner_id
where io.issotrx = 'N' and trunc(io.dateacct) <= $P{DateEff}
and p.isstocked = 'Y' -- Services do not changes FAR
and ( io.ad_client_id = $P{AD_Client_ID} or $P{AD_Client_ID} = 0)
and ( io.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} = 0)
and io.DOCSTATUS in ('CO','CL','RE','VO')
and (bp.c_BPartner_id = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null or $P{C_BPartner_ID}  = 0)
and iol.movementqty <> 0 -- to avoid divide by zero
and bp.IsFAR = 'Y'
and iol.IsReconciled = 'N'
and (iol.MOVEMENTQTY - nvl((select(nvl(sum(mi.qty),0))
	from M_MATCHINV mi where (trunc(mi.dateacct) <= $P{DateEff} or mi.dateacct is null)
	 and mi.M_INOUTLINE_ID = iol.M_INOUTLINE_ID and mi.posted = 'Y'
	group by mi.M_INOUTLINE_ID),0)) <> 0
order by bp.value,pc.value,p.value]]>
