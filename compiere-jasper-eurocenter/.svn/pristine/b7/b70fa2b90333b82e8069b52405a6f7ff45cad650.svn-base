	<queryString>
		<![CDATA[SELECT NVL(
  ( SELECT name FROM ad_client WHERE ad_client_id = $P{AD_Client_ID}
  ), ' ' ) AS L_CLIENT,
  NVL(
  ( SELECT name FROM ad_org WHERE ad_org_id = $P{AD_Org_ID}
  ), ' ' ) AS L_ORG,
  NVL(
  ( SELECT name FROM c_bp_group WHERE c_bp_group_id = $P{C_BP_Group_ID}
  ), ' ' ) AS L_GROUP,
  NVL(
  (SELECT name FROM c_bpartner WHERE c_bpartner_id = $P{C_BPartner_ID}
  ), ' ' ) AS L_BP,
  bp.value AS bpvalue,
  bp.name  AS name,bpg.Name as GroupName,
  bp.SO_creditlimit creditLimit,
  'I'                                                                         AS doctype,
  i.documentno                                                                AS documentno,
  i.poreference                                                               AS poreference,
  TRUNC(i.dateinvoiced)                                                       AS dateinvoiced,
  TRUNC(i.dateacct)                                                           AS dateacct,
  i.isindispute                                                               AS indispute,
  PaymentTermDuedays(i.c_PaymentTerm_ID, i.DateInvoiced, TRUNC($P{DateFrom})) AS dueday,
  NVL(
  (SELECT TRUNC(l.DueDate)
  FROM C_InvoicePaySchedule l
  WHERE l.C_Invoice_ID=i.C_Invoice_ID
  AND rownum          = 1
  ),TRUNC(i.dateinvoiced - PaymentTermDueDays(i.C_PaymentTerm_ID,i.DateInvoiced, i.DateInvoiced))) AS duedate,
  Z_INVOICE_OPEN_ASOF_BASE_ACTSM(i.C_Invoice_ID,0,TRUNC($P{DateFrom}),$P{C_AcctSchema_ID})         AS docamt ,
  Z_INVOICE_OPEN_ASOF_BASE_ACTSM(i.C_Invoice_ID,0,TRUNC($P{DateFrom}),$P{C_AcctSchema_ID})         AS grandtotalbase,
  Z_INVOICE_OPEN_ASOF(i.C_Invoice_ID,0,TRUNC($P{DateFrom}))                                        AS grandtotal,
  0                                                                                                AS payamt,
  NULL                                                                                             AS C_InvoicePaySchedule_ID,
  NVL(
  ( SELECT AD_LANGUAGE FROM ad_client WHERE ad_client_id = $P{AD_Client_ID}
  ), 'en_US' ) AS AD_LANGUAGE,
  cur.Iso_Code,
  case when i.DOUBTFULDATE > TRUNC($P{DateFrom}) then TO_CHAR('N') else TO_CHAR(i.IsDoubtful) end  AS ISDOUBTFUL, i.DiscountDate, (Select oo.Z_REFCHANTIER from C_Order oo where oo.C_Order_ID = i.C_Order_ID) as REFCHANTIER, i.GrandTotal as GT
FROM c_invoice i
INNER JOIN c_bpartner bp
ON ( i.c_bpartner_id = bp.c_bpartner_id)
INNER JOIN c_bp_group bpg
ON ( bp.C_BP_Group_ID = bpg.C_BP_Group_ID )
INNER JOIN c_currency cur
ON ( i.C_Currency_ID                                                                          = cur.C_Currency_ID )
WHERE ( bpg.Value                                                                            >= $P{BP_GROUP_FROM}
OR $P{BP_GROUP_FROM}                                                                         IS NULL
OR $P{BP_GROUP_FROM}                                                                          = '' )
AND ( bpg.Value                                                                              <= $P{BP_GROUP_TO}
OR $P{BP_GROUP_TO}                                                                           IS NULL
OR $P{BP_GROUP_TO}                                                                            = '' )
AND ( bp.C_BPartner_ID                                                                        = $P{C_BPartner_ID}
OR $P{C_BPartner_ID}                                                                         IS NULL
OR $P{C_BPartner_ID}                                                                          = 0 )
AND ( bp.C_BP_Group_ID                                                                        = $P{C_BP_Group_ID}
OR $P{C_BP_Group_ID}                                                                         IS NULL
OR $P{C_BP_Group_ID}                                                                          = 0 )
AND ( ( i.issotrx                                                                             = 'Y'
AND $P{IsSOTrx}                                                                               = 'Y' )
OR ( i.issotrx                                                                                = 'N'
AND $P{IsSOTrx}                                                                               = 'N' ) )

AND (($P{IsDoubtfulIncluded} = 'O' and i.IsDoubtful = 'Y' and i.DOUBTFULDATE <= TRUNC($P{DateFrom}))
OR ($P{IsDoubtfulIncluded} = 'Y' )
OR ($P{IsDoubtfulIncluded} = 'N'  and (i.IsDoubtful = 'N'  or i.DOUBTFULDATE > TRUNC($P{DateFrom}))) )

AND i.docstatus                                                                              IN ('CO','CL','RE','VO')
AND ( i.AD_Client_ID                                                                          = $P{AD_Client_ID}
OR $P{AD_Client_ID}                                                                          IS NULL
OR $P{AD_Client_ID}                                                                           = 0 )
AND ( i.AD_Org_ID                                                                             = $P{AD_Org_ID}
OR $P{AD_Org_ID}                                                                             IS NULL
OR $P{AD_Org_ID}                                                                              = 0 )
AND TRUNC(i.dateacct)                                                                        <= TRUNC($P{DateFrom})
AND Z_INVOICE_OPEN_ASOF_BASE_ACTSM(i.C_Invoice_ID,0,TRUNC($P{DateFrom}),$P{C_AcctSchema_ID}) <> 0
AND i.Posted                                                                                  ='Y'
AND (($P{Financement} = 'N' AND i.IsFinancement = 'N') OR ($P{Financement} = 'F' AND i.IsFinancement = 'Y') OR ($P{Financement} = 'Y'))
UNION ALL
SELECT NVL(
  ( SELECT name FROM ad_client WHERE ad_client_id = $P{AD_Client_ID}
  ), ' ' ) AS L_CLIENT,
  NVL(
  ( SELECT name FROM ad_org WHERE ad_org_id = $P{AD_Org_ID}
  ), ' ' ) AS L_ORG,
  NVL(
  (SELECT name FROM c_bp_group WHERE c_bp_group_id = $P{C_BP_Group_ID}
  ), ' ' ) AS L_GROUP,
  NVL(
  (SELECT name FROM c_bpartner WHERE c_bpartner_id = $P{C_BPartner_ID}
  ), ' ' ) AS L_BP,
  bp.value AS bpvalue,
  bp.name  AS name,bpg.Name as GroupName,
  bp.SO_creditlimit creditLimit,
  'P' AS docType,
  p.documentno
  || '          '
  || p.c_payment_id                    AS documentno,
  NULL                                 AS poreference,
  TRUNC(p.datetrx)                     AS dateinvoiced,
  TRUNC(p.dateacct)                    AS dateacct,
  'N'                                  AS indispute,
  0                                    AS dueday,
  TRUNC(COALESCE(p.duedate,p.datetrx)) AS duedate,
  0                                    AS docbase,
  - currencyConvert(ZpaymentAvailable2(p.C_Payment_ID,TRUNC($P{DateFrom}),$P{IsDraftIncluded}),p.C_Currency_ID,
  (SELECT a.C_Currency_ID
  FROM c_acctschema a
  WHERE a.c_acctschema_id= $P{C_AcctSchema_ID}
  ),p.DateAcct,p.C_ConversionType_ID,p.AD_Client_ID,p.AD_Org_ID) AS grandtotalbase,
  ZpaymentAvailable2(p.C_Payment_ID,TRUNC($P{DateFrom}),$P{IsDraftIncluded})          AS grandtotal,
  currencyConvert(ZpaymentAvailable2(p.C_Payment_ID,TRUNC($P{DateFrom}),$P{IsDraftIncluded}),p.C_Currency_ID,
  (SELECT a.C_Currency_ID
  FROM c_acctschema a
  WHERE a.c_acctschema_id = $P{C_AcctSchema_ID}
  ),p.DateAcct,p.C_ConversionType_ID,p.AD_Client_ID,p.AD_Org_ID) AS PayAmt,
  NULL,
  NVL(
  ( SELECT AD_LANGUAGE FROM ad_client WHERE ad_client_id = $P{AD_Client_ID}
  ), 'en_US' ) AS AD_LANGUAGE,
  cur.Iso_Code,
  case when p.DOUBTFULDATE > TRUNC($P{DateFrom}) then TO_CHAR('N') else TO_CHAR(p.IsDoubtful) end AS ISDOUBTFUL, NULL as DiscountDate, (Select oo.Z_REFCHANTIER from C_Order oo where oo.C_Order_ID = p.C_Order_ID) as REFCHANTIER, p.PayAmt as GT
FROM c_Payment p,
  c_bp_group bpg,
  c_bpartner bp,
  C_Currency cur
WHERE ( bpg.Value                                         >= $P{BP_GROUP_FROM}
OR $P{BP_GROUP_FROM}                                      IS NULL
OR $P{BP_GROUP_FROM}                                       = '' )
AND ( bp.C_BP_Group_ID                                     = bpg.C_BP_Group_ID )
AND ( bpg.Value                                           <= $P{BP_GROUP_TO}
OR $P{BP_GROUP_TO}                                        IS NULL
OR $P{BP_GROUP_TO}                                         = '' )
AND ( bp.C_BPartner_ID                                     = $P{C_BPartner_ID}
OR $P{C_BPartner_ID}                                      IS NULL
OR $P{C_BPartner_ID}                                       = 0 )
AND ( bp.C_BP_Group_ID                                     = $P{C_BP_Group_ID}
OR $P{C_BP_Group_ID}                                      IS NULL
OR $P{C_BP_Group_ID}                                       = 0 )
AND ( ( p.IsReceipt                                        = 'Y'
AND $P{IsSOTrx}                                            = 'Y' )
OR ( p.IsReceipt                                           = 'N'
AND $P{IsSOTrx}                                            = 'N' ) )

AND (($P{IsDoubtfulIncluded} = 'O' and p.IsDoubtful = 'Y' and p.DOUBTFULDATE <= TRUNC($P{DateFrom}))
OR ($P{IsDoubtfulIncluded} = 'Y' )
OR ($P{IsDoubtfulIncluded} = 'N'  and (p.IsDoubtful = 'N' or p.DOUBTFULDATE > TRUNC($P{DateFrom}))) )

AND p.docstatus                                           IN ('CO','CL','RE','VO')
AND p.c_bpartner_id                                        = bp.c_bpartner_id
AND (p.C_Charge_ID                                         = 0
OR p.C_Charge_ID                                          IS NULL)
AND (p.IsDraft='N' OR (p.IsDraft='Y' AND $P{IsDraftIncluded}= 'Y')) -- 23439
AND ( p.AD_Client_ID                                       = $P{AD_Client_ID}
OR $P{AD_Client_ID}                                       IS NULL
OR $P{AD_Client_ID}                                        = 0 )
AND ( p.AD_Org_ID                                          = $P{AD_Org_ID}
OR $P{AD_Org_ID}                                          IS NULL
OR $P{AD_Org_ID}                                           = 0 )
AND TRUNC(p.dateacct)                                     <= TRUNC($P{DateFrom})
AND ZpaymentAvailable2(p.C_Payment_ID,TRUNC($P{DateFrom}),$P{IsDraftIncluded}) <> 0
AND p.C_Currency_ID                                        = cur.C_Currency_ID
  -- 20100222 vha jlb sre dho and p.IsAllocated = 'N'
AND p.Posted='Y'
AND $P{Financement} != 'F'
ORDER BY $P!{SORTBYNAME},
  Duedate,
  11,
  documentno]]>
	</queryString>
