<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="SubReport_Org" language="groovy" pageWidth="600" pageHeight="842" columnWidth="600" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" resourceBundle="traduction">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="AD_ORG_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[Select org.name as AD_ORG_NAME,
cl.address1 as AD_ORG_ADDRESS1,
cl.postal as AD_ORG_POSTAL,
cl.city as AD_ORG_CITY,
orgi.phone as AD_ORG_PHONE,
orgi.fax as AD_ORG_FAX,
orgi.telfree
from AD_Org org
inner join ad_orginfo orgi on (orgi.ad_org_id = org.AD_Org_ID)
left join C_Location cl on (cl.c_location_id = orgi.c_location_id)
where org.ad_org_id = $P{AD_ORG_ID}]]>
	</queryString>
	<field name="AD_ORG_NAME" class="java.lang.String"/>
	<field name="AD_ORG_ADDRESS1" class="java.lang.String"/>
	<field name="AD_ORG_POSTAL" class="java.lang.String"/>
	<field name="AD_ORG_CITY" class="java.lang.String"/>
	<field name="AD_ORG_PHONE" class="java.lang.String"/>
	<field name="AD_ORG_FAX" class="java.lang.String"/>
	<field name="TELFREE" class="java.lang.String"/>
	<title>
		<band height="80" splitType="Stretch">
			<textField isBlankWhenNull="false">
				<reportElement key="textField" x="0" y="0" width="441" height="79"/>
				<box>
					<topPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<leftPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<bottomPen lineWidth="0.0" lineColor="#000000"/>
					<rightPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
				</box>
				<textElement lineSpacing="1_1_2">
					<font size="7"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[($F{AD_ORG_NAME} == null ? "" :$F{AD_ORG_NAME}+ '\n') +
($F{AD_ORG_ADDRESS1} == null ? "" :$F{AD_ORG_ADDRESS1}+ '\n' )+
($F{AD_ORG_POSTAL} == null ? "" :$F{AD_ORG_POSTAL}+ ' ' )+
($F{AD_ORG_CITY} == null ? "" :$F{AD_ORG_CITY}+ '\n') +
(msg($R{phone.fax}, $F{AD_ORG_PHONE} == null ? "" :$F{AD_ORG_PHONE}, $F{AD_ORG_FAX} == null ? "" :$F{AD_ORG_FAX}))+ '\n']]></textFieldExpression>
			</textField>
		</band>
	</title>
</jasperReport>
